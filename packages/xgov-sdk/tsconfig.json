{
  "compilerOptions": {
    "outDir": "./bin",
    "experimentalDecorators": true,
    "declaration": true,
    "target": "ES2022",
    "moduleResolution": "node",
    "module": "ES2022",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  }
}
